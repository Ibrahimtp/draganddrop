<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.datatables.net/1.12.1/css/jquery.dataTables.min.css">
    <style>
        *{
            padding: 5px;
        }
    </style>
    <title>Document</title>
</head>
<body>
    



<div class="container 75">
 
    <table id="myTable" class="table table-responsive table-bordered table-sort table-arrows remember-sort">
        <tr class="p-5">
            <th>Fullname</th>
            <th class="order-by-desc ">Year</th>
            <th>Surat&Scores</th>
            
        </tr>

        <% for (student of allStudent) { %> 
            <tr>
                <td><%= student.fullName %></td>
                <td><%= student.grade.replace('YEAR','') %></td>
                <td> 

                    <% if(student.scores) { %>
                        <table class="table table-responsive table-bodered table-striped">
                           <tr>
                            <th>Surat</th>
                            <th>Verse</th>
                            <th>Scores</th>
                           </tr>

                           <% for (score of student.scores) { %>
                            <tr>
                                <td><%= score.surat %></td>
                                <td><%= score.range.replace('Verse :','') %></td>
                                <td><%= score.score %></td>
                            </tr>
                            <% } %>



                        </table>
                            <% } %>
                </td>
            </tr>
            <% } %>



    </table>



</div>



















    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>
